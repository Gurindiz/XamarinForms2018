<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:Master="clr-namespace:App02_TipoPaginaXF.TipoPagina.Master"
             x:Class="App02_TipoPaginaXF.TipoPagina.Master.Master">
    <MasterDetailPage.Master>
        <!--Menu-->
        <ContentPage Title="Menu">
            <StackLayout BackgroundColor="DarkGoldenrod">
                <Button Text="Pagina 1" BackgroundColor="Transparent" Clicked="MudarPagina1"/>
                <Button Text="Pagina 2" BackgroundColor="Transparent" Clicked="MudarPagina2"/>
                <Button Text="Conteudo" BackgroundColor="Transparent" Clicked="MudarPagina3"/>
                <Button Text="Buscar CEP" BackgroundColor="Transparent" Clicked="MudarBuscarCep"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <!--Conteudo-->

        <Master:Conteudo/>
    </MasterDetailPage.Detail>
</MasterDetailPage>